  "editor.fontFamily": "Hack, Julia Mono, Droid Sans Mono, DejaVu Code, mononoki, Inconsolata, Geist Mono, Fira Code, Maple Mono, JetBrains Mono, Menlo, Monaco, 'Courier New', monospace",
  "workbench.tree.indent": 18,
  "material-icon-theme.hidesExplorerArrows": true,
  "workbench.tree.renderIndentGuides": "onHover",
  "github.copilot.nextEditSuggestions.enabled": true,
  "path-intellisense.autoTriggerNextSuggestion": true,
  "editor.inlineSuggest.suppressSuggestions": true,
  "debug.console.acceptSuggestionOnEnter": "on",
  "editor.autoIndent": "advanced",
  "indenticator.inner.showIndentGuide": true,
  "markdown.extension.tableFormatter.normalizeIndentation": true,
  "guides.overrideDefault": true,
  "gitlens.views.scm.grouped.views": {
    "repositories": true,
    "searchAndCompare": true,
    "launchpad": true
  },
  "guides.active.width": 0.25,
  "guides.limit.maximum": -1,
  "guides.stack.style": "dotted",
  "guides.normal.width": 0.25,
  "guides.stack.width": 0.5,
  "editor.fontSize": 16,
  "todo-tree.highlights.useColourScheme": true,
  "vim.highlightedyank.enable": true,
  "editor.formatOnPaste": false,
  "editor.formatOnSave": false,
  "editor.formatOnType": false,
  "terminal.integrated.fontLigatures.enabled": true,
  "terminal.integrated.fontFamily": "Fira Code, Maple Mono, Hack Nerd Font",
  "terminal.integrated.fontSize": 14,
  "git.terminalGitEditor": true,
  "terminal.integrated.suggest.enabled": true,
  "terminal.integrated.smoothScrolling": true,
  "terminal.integrated.stickyScroll.enabled": true,
  "terminal.integrated.enableImages": true,
  "terminal.integrated.enableVisualBell": true,
  "terminal.integrated.mouseWheelZoom": true,
  "editor.mouseWheelZoom": true,
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "terminalAllInOne.terminalTheme": "Atelier Cave",
  "julia.executablePath": "D:\\CommonSoftwares\\ProductiveCodingEditors\\Julia\\bin\\julia.exe",
  "julia.execution.inlineResultsForCellEvaluation": true,
  "julia.execution.codeInREPL": true,
  "vim.autoSwitchInputMethod.enable": true,
  "vim.camelCaseMotion.enable": true,
  "vim.changeWordIncludesWhitespace": true,
  "vim.easymotion": true,
  "vim.statusBarColorControl": true,
  "vim.enableNeovim": true,
  "vim.sneak": true,
  "vim.smartRelativeLine": true,
  "vim.targets.enable": true,
  "vim.useSystemClipboard": true,
  "powershell.buttons.showPanelMovementButtons": true,
  "powershell.codeFormatting.autoCorrectAliases": true,
  "powershell.codeFormatting.avoidSemicolonsAsLineTerminators": true,
  "editor.scrollbar.horizontalScrollbarSize": 88,
  "editor.scrollbar.verticalScrollbarSize": 5,
  "editor.wordWrapColumn": 150,
  "vim.textwidth": 1

        {
          "scope": "function",
          "settings": {
            "foreground": "#0a0ae5",
            "fontStyle": "bold underline"
          }
        },
        {
          "scope": "number",
          "settings": {
            "foreground": "#c4570e",
            "fontStyle": "bold"
          }
        }
      ]
    },

{
    "editor.guides.bracketPairs": "active",
    "BracketHighlighter.activeInDebugMode": false,
    "prettier.bracketSameLine": true,
    "txtsyntax.enableAutoImport": true,
    "txtsyntax.enableHighlightLine": true,
    "txtsyntax.showHighlightSidebar": true,
    "editor.tokenColorCustomizations": {
        "[GitHub Light Colorblind (Beta)]": {
            "textMateRules": [
                {
                    "scope": "keyword",
                    "settings": {
                        "foreground": "#d73a49",
                        "fontStyle": "italic"
                    }
                },
                {
                    "scope": "comment",
                    "settings": {
                        "foreground": "#b4c3d2",
                        "fontStyle": "italic"
                    }
                },
                {
                    "scope": "string",
                    "settings": {
                        "foreground": "#144989"
                    }
                },
                {
                    "scope": "type",
                    "settings": {
                        "foreground": "#618dbf",
                        "fontStyle": "bold"
                    }
                },
                {
                    "scope": "function",
                    "settings": {
                        "foreground": "#0a0ae5",
                        "fontStyle": "bold underline"
                    }
                },
                {
                    "scope": "number",
                    "settings": {
                        "foreground": "#c4570e",
                        "fontStyle": "bold"
                    }
                }
            ]
        },
        "[Dainty – City Lights]": {
            "textMateRules": [
                {
                    "scope": "keyword",
                    "settings": {
                        "foreground": "#d73a49",
                        "fontStyle": "italic"
                    }
                },
                {
                    "scope": "comment",
                    "settings": {
                        "foreground": "#213c56",
                        "fontStyle": "italic"
                    }
                },
                {
                    "scope": "string",
                    "settings": {
                        "foreground": "#59708a"
                    }
                },
                {
                    "scope": "entity.name.type",
                    "settings": {
                        "foreground": "#618dbf",
                        "fontStyle": "bold"
                    }
                },
                {
                    "scope": "entity.name.function",
                    "settings": {
                        "foreground": "#0a0ae5",
                        "fontStyle": "bold underline"
                    }
                },
                {
                    "scope": "number",
                    "settings": {
                        "foreground": "#c4570e",
                        "fontStyle": "bold"
                    }
                }
            ]
        },
        "[Dainty – Dark+]": {
            "textMateRules": [
                {
                    "scope": "keyword",
                    "settings": {
                        "foreground": "#d73a49",
                        "fontStyle": "italic"
                    }
                },
                {
                    "scope": "comment",
                    "settings": {
                        "foreground": "#213c56",
                        "fontStyle": "italic"
                    }
                },
                {
                    "scope": "string",
                    "settings": {
                        "foreground": "#59708a"
                    }
                },
                {
                    "scope": "entity.name.type",
                    "settings": {
                        "foreground": "#618dbf",
                        "fontStyle": "bold"
                    }
                },
                {
                    "scope": "entity.name.function",
                    "settings": {
                        "foreground": "#2c679b",
                        "fontStyle": "bold underline"
                    }
                },
                {
                    "scope": "number",
                    "settings": {
                        "foreground": "#c4570e",
                        "fontStyle": "bold"
                    }
                }
            ]
        },
        "comments": "",
        "textMateRules": []
    },
    "editor.fontFamily": "Fira Code, Droid Sans Mono, Consolas, 'Courier New', monospace"
}